@{
    ViewData["Title"] = "Indicadores";
}

<h1>@ViewData["Title"]</h1>

<div class="container my-4 mb-2" id="conteudo-cards"></div>


@section Scripts {
    <script>
        $(document).ready(() => {
            $.ajax({
                url: '@Url.Action("CardsResultados", "Admin")',
                dataType: 'html',
                success: (resp) => {
                    $('#conteudo-cards').html(resp);
                }
            });
        });
    </script>
}